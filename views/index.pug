                        a.social-media__button(href="https://www.twitter.com/artisanclick") 
                            i.fa.fa-twitter
                        a.social-media__button(href="https://www.codepen.io/artisanclick") 
                            i.fa.fa-codepen
                        a.social-media__button(href="https://github.com/artisanclick") 
