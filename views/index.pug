    .about.section#about(data-matching-link="#about-link")
        .wrapper
            img.about__pic(src="../src/assets/images/profilepic.jpg" alt="that's me!")
            .heading
                span.heading__sub.heading--uppercase About Section
                h2.heading__main Your everyday geeky gal
                //- h3.heading__lead Ambitious, persistent, self-taught
            p.about__text.full-width-text Hiya, I'm an aspiring full stack developer, #[span.mark w]ith extensive knowledge of HT#[span.mark M]L, CSS and JS, and working knowledge of Angular, hoping to join a fast-growing and innovative company as a passionate junior developer and grow my portfolio. 
            .justified.grid-x
                .cell.medium-6 #[span.icon.icon--500--other-location.icon--alt--ib] Chesterfield, Derbyshire
                .cell.medium-6.social-media 
                    a.social-media__button(href="https://www.twitter.com/artisanclick") 
                        i.fa.fa-twitter
                    a.social-media__button(href="https://www.instagram.com/artisanclick") 
                        i.fa.fa-instagram
                    a.social-media__button(href="https://open.spotify.com/user/jg78z1akyh5ulo6cu2ss4lv6v?si=inyh5SIwR2amlWO7uXFQBg") 
                        i.fa.fa-spotify
                    a.social-media__button(href="https://www.last.fm/user/artisanclick") 
                        i.fa.fa-lastfm
    .contact.section.section--extra-b-padding#contact(data-matching-link="#contact-link")
        .wrapper
            .heading
                span.heading__sub.heading--uppercase Contact Section
                h2.heading__main.heading__main--light.heading__main--extra-margin Like what you see, get in touch!
                //- h3.heading__lead.heading__lead--light Lorem ipsum dolor sit amet consectetur adipisicing elit. Repellat, quia!
            //- form(action="POST")
            form.grid-x.grid-margin-x(method="POST" action="/send")
                label.cell.small-6 Name
                    input(type="text" name="name" id="name" placeholder="Your Name")
                label.cell.small-6 Phone
                    input(type="text" name="number" id="number" placeholder="Your Phone Nmber")
                label.cell Email
                    input(type="email" name="email" id="email" placeholder="Your Email")
                label.cell Message
                    textarea(placeholder="Your message")
                .cell.grid-x
                    .cell.medium-6
                        button.button(type="submit") Send Message
                    .cell.medium-6.social-media
                        a.social-media__button(href="mailto:artisanclickinfo@gamil.com") 
